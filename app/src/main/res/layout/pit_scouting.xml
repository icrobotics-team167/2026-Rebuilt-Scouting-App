<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:weightSum="64"
        android:background="@color/background"
        android:orientation="horizontal">

        <View
            android:id="@+id/divider31"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:background="?android:attr/listDivider" />

        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="62"
            android:orientation="vertical"
            android:weightSum="68">

            <View
                android:id="@+id/divider33"
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="1"
                android:background="?android:attr/listDivider" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="8"
                android:orientation="horizontal"
                android:weightSum="48">

                <TextView
                    android:id="@+id/teamNumberHeader"
                    style="@style/AutoResize25"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="7.5"
                    android:gravity="center_vertical"
                    android:paddingStart="10dp"
                    android:text="Team Number:" />

                <EditText
                    android:id="@+id/teamNumber"
                    style="@style/AutoResize25"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="3.6"
                    android:ems="10"
                    android:inputType="number"
                    android:hint="#" />

                <TextView
                    android:id="@+id/scouterHeader"
                    style="@style/AutoResize25"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="4"
                    android:paddingStart="10dp"
                    android:gravity="center_vertical"
                    android:text="Name:" />

                <EditText
                    android:id="@+id/scouter"
                    style="@style/AutoResize20"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="6"
                    android:ems="10"
                    android:inputType="text"
                    android:hint="Your name"
                    android:text="" />

                <TextView
                    android:id="@+id/unitSpinnerHeader"
                    style="@style/AutoResize25"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="4"
                    android:gravity="center_vertical"
                    android:paddingStart="10dp"
                    android:text="Units:" />

                <Spinner
                    android:id="@+id/unitSpinner"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="6" />

                <Button
                    android:id="@+id/editButton"
                    style="@style/AutoResize20"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="5"
                    android:layout_gravity="center"
                    android:backgroundTint="@color/secondaryForeground"
                    android:textColor="@color/black"
                    android:text="Edit Old Data" />

                <Space
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1" />

                <Button
                    android:id="@+id/exportButton"
                    style="@style/AutoResize20"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="5"
                    android:layout_gravity="center"
                    android:backgroundTint="@color/secondaryForeground"
                    android:textColor="@color/black"
                    android:text="Export All Data" />

                <Space
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1" />

                <Button
                    android:id="@+id/exportButton2"
                    style="@style/AutoResize20"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_gravity="center"
                    android:layout_weight="5"
                    android:backgroundTint="@color/red"
                    android:text="Delete Data"
                    android:textColor="@color/white" />

            </LinearLayout>

            <View
                android:id="@+id/divider25"
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="1"
                android:background="?android:attr/listDivider" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="7"
                android:orientation="horizontal"
                android:weightSum="48">

                <TextView
                    android:id="@+id/drivebaseHeader"
                    style="@style/AutoResize25"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="6"
                    android:gravity="center|start"
                    android:paddingStart="10dp"
                    android:text="Drivebase:" />

                <CheckBox
                    android:id="@+id/swerve"
                    style="@style/AutoResize20"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="6"
                    android:text="Swerve" />

                <CheckBox
                    android:id="@+id/bump"
                    style="@style/AutoResize20"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="8"
                    android:text="Bump" />

                <CheckBox
                    android:id="@+id/trench"
                    style="@style/AutoResize20"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="7"
                    android:text="Trench" />

                <TextView
                    android:id="@+id/botDimensionsHeader"
                    style="@style/AutoResize20"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="8"
                    android:gravity="center_vertical"
                    android:paddingStart="10dp"
                    android:text="Bot Dimensions:" />

                <EditText
                    android:id="@+id/botDimensionsWidth"
                    style="@style/AutoResize20"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="3"
                    android:ems="10"
                    android:gravity="center"
                    android:hint="W"
                    android:inputType="numberDecimal"
                    android:paddingTop="10dp"
                    android:text="" />

                <TextView
                    android:id="@+id/botX1"
                    style="@style/AutoResize20"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:gravity="center_horizontal|bottom"
                    android:paddingBottom="10dp"
                    android:text="x" />

                <EditText
                    android:id="@+id/botDimensionsDepth"
                    style="@style/AutoResize20"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="3"
                    android:ems="10"
                    android:gravity="center"
                    android:hint="D"
                    android:inputType="numberDecimal"
                    android:paddingTop="10dp"
                    android:text="" />

                <TextView
                    android:id="@+id/botX2"
                    style="@style/AutoResize20"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:gravity="center_horizontal|bottom"
                    android:paddingBottom="10dp"
                    android:text="x" />

                <EditText
                    android:id="@+id/botDimensionsHeight"
                    style="@style/AutoResize20"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="3"
                    android:ems="10"
                    android:gravity="center"
                    android:hint="H"
                    android:inputType="numberDecimal"
                    android:paddingTop="10dp"
                    android:text="" />

            </LinearLayout>

            <View
                android:id="@+id/divider26"
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="1"
                android:background="?android:attr/listDivider" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="7"
                android:orientation="horizontal"
                android:weightSum="48">

                <TextView
                    android:id="@+id/intakeHeader"
                    style="@style/AutoResize25"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="6"
                    android:gravity="center|start"
                    android:paddingStart="10dp"
                    android:text="Intake:" />

                <CheckBox
                    android:id="@+id/humanIntake"
                    style="@style/AutoResize20"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="6"
                    android:text="Human" />

                <CheckBox
                    android:id="@+id/throughBumperIntake"
                    style="@style/AutoResize20"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="8"
                    android:text="Through Bumper" />

                <CheckBox
                    android:id="@+id/overBumperIntake"
                    style="@style/AutoResize20"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="7"
                    android:text="Over Bumper" />

                <TextView
                    android:id="@+id/textView4"
                    style="@style/AutoResize20"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="8"
                    android:gravity="center_vertical"
                    android:paddingStart="10dp"
                    android:text="Intake Width:" />

                <EditText
                    android:id="@+id/intakeWidth"
                    style="@style/AutoResize20"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="3"
                    android:ems="10"
                    android:gravity="center"
                    android:hint="W"
                    android:inputType="numberDecimal"
                    android:paddingTop="10dp"
                    android:text="" />

            </LinearLayout>

            <View
                android:id="@+id/divider44"
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="1"
                android:background="?android:attr/listDivider" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="7"
                android:orientation="horizontal"
                android:weightSum="48">

                <TextView
                    android:id="@+id/hopperHeader"
                    style="@style/AutoResize25"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="6"
                    android:gravity="center_vertical"
                    android:paddingStart="10dp"
                    android:text="Hopper:" />

                <CheckBox
                    android:id="@+id/openHopper"
                    style="@style/AutoResize20"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="6"
                    android:text="Open" />

                <CheckBox
                    android:id="@+id/extendableHopper"
                    style="@style/AutoResize20"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="8"
                    android:text="Extendable" />

                <Space
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="7" />

                <TextView
                    android:id="@+id/textView5"
                    style="@style/AutoResize20"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="8"
                    android:gravity="center_vertical"
                    android:paddingStart="10dp"
                    android:text="Dimensions:" />

                <EditText
                    android:id="@+id/hopperDimensionsWidth"
                    style="@style/AutoResize20"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="3"
                    android:ems="10"
                    android:gravity="center"
                    android:hint="W"
                    android:inputType="numberDecimal"
                    android:paddingTop="10dp"
                    android:text="" />

                <TextView
                    android:id="@+id/hopperX1"
                    style="@style/AutoResize20"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:gravity="center_horizontal|bottom"
                    android:paddingBottom="10dp"
                    android:text="x" />

                <EditText
                    android:id="@+id/hopperDimensionsDepth"
                    style="@style/AutoResize20"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="3"
                    android:ems="10"
                    android:gravity="center"
                    android:hint="D"
                    android:inputType="numberDecimal"
                    android:paddingTop="10dp"
                    android:text="" />

                <TextView
                    android:id="@+id/hopperX3"
                    style="@style/AutoResize20"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:gravity="center_horizontal|bottom"
                    android:paddingBottom="10dp"
                    android:text="x" />

                <EditText
                    android:id="@+id/hopperDimensionsHeight"
                    style="@style/AutoResize20"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="3"
                    android:ems="10"
                    android:gravity="center"
                    android:hint="H"
                    android:inputType="numberDecimal"
                    android:paddingTop="10dp"
                    android:text="" />

            </LinearLayout>

            <View
                android:id="@+id/divider27"
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="1"
                android:background="?android:attr/listDivider" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="7"
                android:orientation="horizontal"
                android:weightSum="48">

                <TextView
                    android:id="@+id/turretHeader"
                    style="@style/AutoResize25"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:gravity="center|start"
                    android:layout_weight="6"
                    android:paddingStart="10dp"
                    android:text="Shooter:" />

                <CheckBox
                    android:id="@+id/tiltTurret"
                    style="@style/AutoResize20"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="6"
                    android:text="Tilt" />

                <CheckBox
                    android:id="@+id/turnTurret"
                    style="@style/AutoResize20"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="8"
                    android:text="Turn" />

                <Space
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="7" />

                <TextView
                    android:id="@+id/numberOfShootersHeader"
                    style="@style/AutoResize20"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="10"
                    android:gravity="center_vertical"
                    android:paddingStart="10dp"
                    android:text="Number of Shooters:" />

                <Space
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="0.5" />

                <EditText
                    android:id="@+id/numberOfShooters"
                    style="@style/AutoResize25"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="2"
                    android:ems="10"
                    android:gravity="center"
                    android:inputType="number"
                    android:hint="#"
                    android:text="" />
            </LinearLayout>

            <View
                android:id="@+id/divider28"
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="1"
                android:background="?android:attr/listDivider" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="5"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/cornOnCobHeader"
                    style="@style/AutoResize25"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:gravity="center_vertical"
                    android:paddingStart="10dp"
                    android:text="What Do You Like On Your Corn?" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_weight="5"
                android:layout_height="0dp"
                android:weightSum="6"
                android:paddingStart="10dp"
                android:orientation="horizontal">

                <CheckBox
                    android:id="@+id/yesCornOnCob"
                    style="@style/AutoResize20"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:text="Salt" />

                <CheckBox
                    android:id="@+id/definitelyCornOnCob"
                    style="@style/AutoResize20"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:text="Pepper" />

                <CheckBox
                    android:id="@+id/absolutelyCornOnCob"
                    style="@style/AutoResize20"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:text="Butter" />

                <CheckBox
                    android:id="@+id/checkBox6"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="0.2"
                    android:text="" />

                <EditText
                    android:id="@+id/editTextText"
                    style="@style/AutoResize20"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="2"
                    android:ems="10"
                    android:paddingTop="5dp"
                    android:inputType="text"
                    android:hint="Other" />
            </LinearLayout>

            <View
                android:id="@+id/divider35"
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="1"
                android:background="?android:attr/listDivider" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="6"
                android:weightSum="48"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/teamRatingHeader"
                    style="@style/AutoResize25"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="7"
                    android:gravity="center_vertical"
                    android:paddingStart="10dp"
                    android:text="Team Rating:" />

                <Spinner
                    android:id="@+id/teamRating"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="7" />
            </LinearLayout>

            <View
                android:id="@+id/divider29"
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="1"
                android:background="?android:attr/listDivider" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="7"
                android:weightSum="12"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/commentsHeader"
                    style="@style/AutoResize30"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:gravity="center"
                    android:layout_weight="1.5"
                    android:text="Notes:" />

                <EditText
                    android:id="@+id/comments"
                    style="@style/AutoResize25"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="8.5"
                    android:ems="10"
                    android:inputType="text"
                    android:hint="Notes go here..." />

                <Button
                    android:id="@+id/saveExitButton"
                    style="@style/AutoResize25"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="2"
                    android:backgroundTint="@color/primaryForeground"
                    android:textColor="@color/white"
                    android:text="Save and Exit" />
            </LinearLayout>

            <View
                android:id="@+id/divider34"
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="1"
                android:background="?android:attr/listDivider" />

        </LinearLayout>

        <View
            android:id="@+id/divider32"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:background="?android:attr/listDivider" />
    </LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>