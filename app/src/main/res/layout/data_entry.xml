<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        android:weightSum="120"
        android:background="?attr/colorSurface"
        android:id="@+id/dataEntry"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="10"
            android:weightSum="12"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/teamNumberHeader"
                style="@style/AutoResize25"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:gravity="center"
                android:layout_weight="1"
                android:text="@string/data_entry_team_number_header" />

            <EditText
                android:id="@+id/teamNumber"
                style="@style/AutoResize25"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:gravity="center"
                android:paddingTop="10dp"
                android:layout_weight="1"
                android:hint="#" />

            <Space
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1" />

            <TextView
                android:id="@+id/matchNumberHeader"
                style="@style/AutoResize25"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:gravity="center"
                android:text="@string/data_entry_match_number_header" />

            <TextView
                android:id="@+id/matchNumber"
                style="@style/AutoResize25"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="2"
                android:gravity="center_vertical"
                android:hint="#" />

            <TextView
                android:id="@+id/scouter"
                style="@style/AutoResize25"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="2.5"
                android:gravity="center_vertical"
                android:text="@string/data_entry_scouter_name" />

            <TextView
                android:id="@+id/scoutingAssignment"
                style="@style/AutoResize25"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1.5"
                android:gravity="center_vertical"
                android:text="@string/data_entry_scouting_assignment" />

            <ImageView
                android:id="@+id/schoolLogo"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:src="?attr/SchoolLogo" />

            <ImageView
                android:id="@+id/teamLogo"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:src="@drawable/team_167_logo" />

        </LinearLayout>

        <View
            android:id="@+id/divider14"
            android:layout_width="match_parent"
            android:layout_weight="3"
            android:layout_height="0dp"
            android:foreground="?attr/dividerColor" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="40"
            android:orientation="horizontal"
            android:weightSum="64">

            <View
                android:id="@+id/divider10"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:foreground="?attr/dividerColor" />

            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="20"
                android:orientation="vertical"
                android:weightSum="10">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_weight="2"
                    android:orientation="horizontal">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:orientation="vertical">

                        <TextView
                            android:id="@+id/autoHeader"
                            style="@style/AutoResize25"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:gravity="center_vertical"
                            android:paddingStart="10dp"
                            android:text="@string/data_entry_auto_header" />
                    </LinearLayout>
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_weight="5"
                    android:orientation="horizontal">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:orientation="vertical"
                        android:layout_weight="2"
                        android:weightSum="6">

                        <TextView
                            android:id="@+id/autoCyclesHeader"
                            style="@style/AutoResize20"
                            android:layout_width="match_parent"
                            android:layout_height="0dp"
                            android:layout_weight="2"
                            android:gravity="center_horizontal"
                            android:layout_gravity="center_vertical"
                            android:text="@string/data_entry_cycles_header" />

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="0dp"
                            android:layout_weight="4"
                            android:weightSum="5"
                            android:orientation="horizontal">

                            <Button
                                android:id="@+id/autoDecButton"
                                style="@style/AutoResize60"
                                android:layout_width="0dp"
                                android:layout_height="match_parent"
                                android:layout_weight="2"
                                android:backgroundTint="?attr/colorSecondary"
                                android:textColor="@color/plusMinusColor"
                                android:text="-" />

                            <TextView
                                android:id="@+id/autoCycles"
                                style="@style/AutoResize25"
                                android:layout_width="0dp"
                                android:layout_height="match_parent"
                                android:gravity="center"
                                android:layout_weight="1"
                                android:text="0" />

                            <Button
                                android:id="@+id/autoIncButton"
                                style="@style/AutoResize45"
                                android:layout_width="0dp"
                                android:layout_height="match_parent"
                                android:backgroundTint="?attr/colorSecondary"
                                android:textColor="@color/plusMinusColor"
                                android:layout_weight="2"
                                android:text="+" />
                        </LinearLayout>
                    </LinearLayout>
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_weight="3"
                    android:orientation="horizontal">

                    <CheckBox
                        style="@style/AutoResize20"
                        android:id="@+id/autoNeutralZone"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_gravity="center_vertical"
                        android:text="@string/data_entry_auto_neutral_zone" />
                </LinearLayout>
            </LinearLayout>

            <View
                android:id="@+id/divider9"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                
                android:foreground="?attr/dividerColor" />

            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="20"
                android:orientation="vertical"
                android:weightSum="10">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_weight="2"
                    android:orientation="horizontal">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:orientation="vertical">

                        <TextView
                            android:id="@+id/activeHubHeader"
                            style="@style/AutoResize25"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:gravity="center_vertical"
                            android:paddingStart="10dp"
                            android:text="@string/data_entry_active_hub_header" />
                    </LinearLayout>
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_weight="5"
                    android:orientation="horizontal">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:orientation="vertical"
                        android:weightSum="6">

                        <TextView
                            style="@style/AutoResize20"
                            android:id="@+id/activeCyclesHeader"
                            android:layout_width="match_parent"
                            android:layout_height="0dp"
                            android:layout_weight="2"
                            android:gravity="center_horizontal"
                            android:layout_gravity="center_vertical"
                            android:text="@string/data_entry_cycles_header" />

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="0dp"
                            android:layout_weight="4"
                            android:weightSum="5"
                            android:orientation="horizontal">

                            <Button
                                android:id="@+id/activeDecButton"
                                style="@style/AutoResize60"
                                android:layout_width="0dp"
                                android:layout_height="match_parent"
                                android:layout_weight="2"
                                android:backgroundTint="?attr/colorSecondary"
                                android:textColor="@color/plusMinusColor"
                                android:text="-" />

                            <TextView
                                android:id="@+id/activeCycles"
                                style="@style/AutoResize25"
                                android:layout_width="0dp"
                                android:layout_height="match_parent"
                                android:gravity="center"
                                android:layout_weight="1"
                                android:text="0" />

                            <Button
                                android:id="@+id/activeIncButton"
                                style="@style/AutoResize45"
                                android:layout_width="0dp"
                                android:layout_height="match_parent"
                                android:layout_weight="2"
                                android:backgroundTint="?attr/colorSecondary"
                                android:textColor="@color/plusMinusColor"
                                android:text="+" />
                        </LinearLayout>
                    </LinearLayout>
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_weight="3"
                    android:orientation="horizontal">

                    <CheckBox
                        android:id="@+id/activePlayedDefense"
                        style="@style/AutoResize20"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_gravity="center_vertical"
                        android:text="@string/data_entry_active_defense" />
                </LinearLayout>
            </LinearLayout>

            <View
                android:id="@+id/divider8"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                
                android:foreground="?attr/dividerColor" />

            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="20"
                android:orientation="vertical"
                android:weightSum="10">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_weight="2"
                    android:orientation="horizontal">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:orientation="vertical">

                        <TextView
                            style="@style/AutoResize25"
                            android:id="@+id/inactiveHubHeader"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:gravity="center_vertical"
                            android:paddingStart="10dp"
                            android:text="@string/data_entry_inactive_hub_header" />
                    </LinearLayout>
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_weight="5"
                    android:orientation="horizontal">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:orientation="vertical"
                        android:weightSum="6">

                        <TextView
                            android:id="@+id/inactiveCyclesHeader"
                            style="@style/AutoResize20"
                            android:layout_width="match_parent"
                            android:layout_height="0dp"
                            android:layout_weight="2"
                            android:gravity="center_horizontal"
                            android:text="@string/data_entry_cycles_header" />

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="0dp"
                            android:layout_weight="4"
                            android:orientation="horizontal">

                            <Button
                                android:id="@+id/inactiveDecButton"
                                style="@style/AutoResize60"
                                android:layout_width="0dp"
                                android:layout_height="match_parent"
                                android:layout_weight="2"
                                android:backgroundTint="?attr/colorSecondary"
                                android:textColor="@color/plusMinusColor"
                                android:text="-" />

                            <TextView
                                android:id="@+id/inactiveCycles"
                                style="@style/AutoResize25"
                                android:layout_width="0dp"
                                android:layout_height="match_parent"
                                android:layout_weight="1"
                                android:gravity="center"
                                android:text="0" />

                            <Button
                                android:id="@+id/inactiveIncButton"
                                style="@style/AutoResize45"
                                android:layout_width="0dp"
                                android:layout_height="match_parent"
                                android:layout_weight="2"
                                android:backgroundTint="?attr/colorSecondary"
                                android:textColor="@color/plusMinusColor"
                                android:text="+" />
                        </LinearLayout>
                    </LinearLayout>
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_weight="3"
                    android:orientation="horizontal">

                    <CheckBox
                        android:id="@+id/inactivePlayedDefense"
                        style="@style/AutoResize20"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:text="@string/data_entry_inactive_defense" />
                </LinearLayout>
            </LinearLayout>

            <View
                android:id="@+id/divider12"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                
                android:foreground="?attr/dividerColor" />

        </LinearLayout>

        <View
            android:id="@+id/divider13"
            android:layout_width="match_parent"
            android:layout_weight="3"
            android:layout_height="0dp"
            
                android:foreground="?attr/dividerColor" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="64"
            android:weightSum="64"
            android:orientation="horizontal">

            <View
                android:id="@+id/divider7"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                
                android:foreground="?attr/dividerColor" />

            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="20"
                android:weightSum="128"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/AllianceScoreHeader"
                    style="@style/AutoResize25"
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_weight="15"
                    android:gravity="center"
                    android:text="@string/data_entry_alliance_score_header" />

                <View
                    android:id="@+id/divider15"
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_weight="2"
                    
                android:foreground="?attr/dividerColor" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_weight="20"
                    android:orientation="horizontal"
                    android:weightSum="5">

                    <TextView
                        android:id="@+id/endAutoHeader"
                        style="@style/AutoResize20"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="2"
                        android:paddingStart="10dp"
                        android:gravity="center_vertical"
                        android:text="@string/data_entry_end_auto_header" />

                    <EditText
                        android:id="@+id/endAuto"
                        style="@style/AutoResize25"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="3"
                        android:ems="10"
                        android:inputType="number"
                        android:hint="#" />

                </LinearLayout>

                <View
                    android:id="@+id/divider16"
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_weight="2"
                    
                android:foreground="?attr/dividerColor" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_weight="20"
                    android:orientation="horizontal"
                    android:weightSum="5">

                    <TextView
                        style="@style/AutoResize20"
                        android:id="@+id/endShift1Header"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="2"
                        android:paddingStart="10dp"
                        android:gravity="center_vertical"
                        android:text="@string/data_entry_end_shift_1_header" />

                    <EditText
                        style="@style/AutoResize25"
                        android:id="@+id/endShift1"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="3"
                        android:ems="10"
                        android:inputType="number"
                        android:hint="#" />
                </LinearLayout>

                <View
                    android:id="@+id/divider17"
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_weight="2"
                    
                android:foreground="?attr/dividerColor" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_weight="20"
                    android:orientation="horizontal"
                    android:weightSum="5">

                    <TextView
                        style="@style/AutoResize20"
                        android:id="@+id/endShift2Header"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="2"
                        android:paddingStart="10dp"
                        android:gravity="center_vertical"
                        android:text="@string/data_entry_end_shift_2_header" />

                    <EditText
                        style="@style/AutoResize25"
                        android:id="@+id/endShift2"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="3"
                        android:ems="10"
                        android:inputType="number"
                        android:hint="#" />
                </LinearLayout>

                <View
                    android:id="@+id/divider18"
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_weight="2"
                    
                android:foreground="?attr/dividerColor" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_weight="20"
                    android:orientation="horizontal"
                    android:weightSum="5" >

                    <TextView
                        style="@style/AutoResize20"
                        android:id="@+id/endShift3Header"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="2"
                        android:paddingStart="10dp"
                        android:gravity="center_vertical"
                        android:text="@string/data_entry_end_shift_3_header" />

                    <EditText
                        style="@style/AutoResize25"
                        android:id="@+id/endShift3"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="3"
                        android:ems="10"
                        android:inputType="number"
                        android:hint="#" />
                </LinearLayout>

                <View
                    android:id="@+id/divider11"
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_weight="2"
                    
                android:foreground="?attr/dividerColor" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_weight="20"
                    android:orientation="horizontal"
                    android:weightSum="5" >

                    <TextView
                        style="@style/AutoResize20"
                        android:id="@+id/endGameHeader"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="2"
                        android:paddingStart="10dp"
                        android:gravity="center_vertical"
                        android:text="@string/data_entry_end_game_header" />

                    <EditText
                        style="@style/AutoResize25"
                        android:id="@+id/endGame"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="3"
                        android:ems="10"
                        android:inputType="number"
                        android:hint="#" />
                </LinearLayout>

                <View
                    android:id="@+id/divider67"
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_weight="3"
                    
                    android:foreground="?attr/dividerColor" />
            </LinearLayout>

            <View
                android:id="@+id/divider4"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                
                android:foreground="?attr/dividerColor" />

            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="41"
                android:orientation="vertical"
                android:weightSum="56">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_weight="14"
                    android:weightSum="71"
                    android:orientation="horizontal">

                    <Space
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1" />

                    <CheckBox
                        android:id="@+id/passedFuel"
                        style="@style/AutoResize20"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="34"
                        android:text="@string/data_entry_passed_fuel" />

                    <View
                        android:id="@+id/divider44"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        
                android:foreground="?attr/dividerColor" />

                    <TextView
                        android:id="@+id/teamRatingHeader"
                        style="@style/AutoResize20"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:gravity="center_vertical"
                        android:paddingStart="10dp"
                        android:layout_weight="15"
                        android:text="@string/data_entry_team_rating_header" />

                    <Spinner
                        android:id="@+id/teamRating"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="20" />

                </LinearLayout>

                <View
                    android:id="@+id/divider22"
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_weight="1"
                    
                android:foreground="?attr/dividerColor" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_weight="14"
                    android:orientation="horizontal"
                    android:weightSum="71">

                    <TextView
                        android:id="@+id/towerLevelHeader"
                        style="@style/AutoResize20"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="15"
                        android:gravity="center_vertical"
                        android:paddingStart="15dp"
                        android:text="@string/data_entry_tower_level_header" />

                    <Spinner
                        android:id="@+id/towerLevel"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="20" />

                    <View
                        android:id="@+id/divider42"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        
                android:foreground="?attr/dividerColor" />

                    <TextView
                        android:id="@+id/towerPositionHeader"
                        style="@style/AutoResize20"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="15"
                        android:gravity="center_vertical"
                        android:paddingStart="10dp"
                        android:text="@string/data_entry_tower_position_header" />

                    <Spinner
                        android:id="@+id/towerPosition"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="20" />

                </LinearLayout>

                <View
                    android:id="@+id/divider21"
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_weight="1"
                    
                android:foreground="?attr/dividerColor" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_weight="25"
                    android:orientation="horizontal"
                    android:weightSum="6">

                    <TextView
                        android:id="@+id/commentsHeader"
                        style="@style/AutoResize30"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:gravity="center"
                        android:text="@string/data_entry_notes_header" />

                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="4"
                        android:gravity="center"
                        android:orientation="vertical">

                        <EditText
                            android:id="@+id/comments"
                            style="@style/AutoResize20"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:ems="10"
                            android:inputType="textMultiLine"
                            android:gravity="bottom"
                            android:text=""
                            android:hint="Your notes go here..." />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:weightSum="2"
                        android:gravity="center"
                        android:orientation="vertical">

                        <Button
                            android:id="@+id/saveExitButton"
                            style="@style/AutoResize20"
                            android:layout_width="match_parent"
                            android:layout_height="0dp"
                            android:layout_weight="1"
                            android:backgroundTint="?attr/colorPrimary"
                            android:text="@string/data_entry_save_exit"
                            android:textColor="?attr/buttonTextPrimary" />
                    </LinearLayout>

                </LinearLayout>

                <View
                    android:id="@+id/divider20"
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_weight="1"
                android:foreground="?attr/dividerColor" />

            </LinearLayout>

            <View
                android:id="@+id/divider24"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:foreground="?attr/dividerColor" />

        </LinearLayout>

    </LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>